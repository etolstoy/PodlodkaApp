CREATE TABLE SqlEpisodeCategory (
  id TEXT NOT NULL PRIMARY KEY,
  name TEXT NOT NULL,
  emoji TEXT NOT NULL,
  episodeIds TEXT NOT NULL
);

insertEpisodeCategory:
INSERT OR REPLACE INTO SqlEpisodeCategory(id, name, emoji, episodeIds)VALUES(?,?,?,?);

selectAll:
SELECT * FROM SqlEpisodeCategory;

selectById:
SELECT * FROM SqlEpisodeCategory WHERE id = ?;